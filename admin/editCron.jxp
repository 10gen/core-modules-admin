<%

if(request._id) {
    var myCron = db.cron.findOne({_id: request._id});
    myCron.name = request.name;
    myCron.url = request.url;
    myCron.xminutes = request.xminutes;
    db.cron.save(myCron);
}
else {
    db.cron.save({name: request.name, url: request.url, xminutes: request.xminutes});
}


%>
